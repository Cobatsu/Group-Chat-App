{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "no-unused-vars":"warn",
        "func-names":"off",
        "object-shorthand":"off",
        "class-methods-use-this":"off",
        "default-case":"off",
        "vars-on-top":"off",
        "no-console":"off",
        "no-underscore-dangle":"off",
        "no-var":"off"
                
    }
}
